NEXT_PUBLIC_HOME_PAGE_DEFAULT_BANNER="/images/home-page-default-banner.jpg"
NEXT_PUBLIC_DEFAULT_PROFILE_IMAGE="/images/default-profile.jpg"
NEXT_PUBLIC_DOMAIN_NAME="http://localhost:3000"
NEXT_PUBLIC_DOMAIN_BANNER_IMAGE="$NEXT_PUBLIC_DOMAIN_NAME/images/home-page-default-banner.jpg"

NEXT_PUBLIC_HOME_PAGE_URL=$NEXT_PUBLIC_DOMAIN_NAME;
# ====== PUBLIC VARIABLES ENDS HERE ======